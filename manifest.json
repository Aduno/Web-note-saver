{
    "manifest_version": 2,
    "name": "Site Comment",
    "description": "Export tabs urls to a file",
    "version": "1.0.0",
    "icons": {"150": "img/mainIcon.png"},
    
    "browser_action": {
        "default_icon": "img/mainIcon.png",
        "default_popup": "popup.html",
        "default_title":"URLGrabber"
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}